{
	"name": "@mjljm/vite-plugin-effect",
	"version": "1.0.0",
	"type": "module",
	"main": "./src/main.ts",
	"module": "./src/main.ts",
	"exports": "./src/main.ts",
	"imports": {
		"#mjljm/vite-plugin-effect/*": {
			"import": "./src/*.ts"
		}
	},
	"packageManager": "pnpm@8.10.2",
	"publishConfig": {
		"access": "restricted",
		"directory": "dist"
	},
	"author": "Jérôme MARTIN",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mjljm/vite-plugin-effect"
	},
	"bugs": {
		"url": "https://github.com/mjljm/vite-plugin-effect/issues"
	},
	"homepage": "https://github.com/mjljm/vite-plugin-effect",
	"description": "",
	"tags": [
		"effect",
		"typescript",
		"algebraic-data-types",
		"functional-programming"
	],
	"keywords": [
		"effect",
		"typescript",
		"algebraic-data-types",
		"functional-programming"
	],
	"scripts": {
		"build": "vite build --ssr src/main.ts",
		"dev": "cross-env DEBUG=true vite-node src/main.ts",
		"circular": "madge --extensions ts --circular --no-color --no-spinner packages/*/src",
		"check": "tsc -b tsconfig.json",
		"lint": "eslint \"packages/*/{src,test,examples}/**/*.ts\"",
		"lint-fix": "pnpm lint --fix"
	},
	"devDependencies": {
		"@effect/language-service": "^0.0.21",
		"@html-eslint/eslint-plugin": "^0.21.0",
		"@html-eslint/parser": "^0.21.0",
		"@mjljm/configs": "workspace:*",
		"@types/eslint": "^8.44.7",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-codegen": "^0.21.0",
		"eslint-plugin-deprecation": "^2.0.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-markdown": "^3.0.1",
		"eslint-plugin-yml": "^1.10.0",
		"fast-check": "^3.14.0",
		"madge": "^6.1.0",
		"npm-check-updates": "^16.14.11",
		"prettier": "^3.1.0",
		"terser": "^5.24.0",
		"typescript": "^5.3.2",
		"vite": "^5.0.3",
		"vite-node": "^0.34.6",
		"vitest": "^0.34.6"
	}
}
